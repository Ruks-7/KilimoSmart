{
  "version": 2,
  "name": "kilimosmart",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": ""
  },
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app"
}
